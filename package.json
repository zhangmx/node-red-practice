  "bugs": {
    "url": "https://github.com/zhangmx/node-red-practice/issues"
  },
  "homepage": "https://github.com/zhangmx/node-red-practice#readme",
  "devDependencies": {
    "jest": "^29.5.0"
  },
  "dependencies": {
    "node-red": "^3.0.2"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "modulePaths": [
    "<rootDir>/src/"
  ]
